      padding: 0 {{ block.settings.desktop_padding }}px;
    }

    .ai-art-showcase-carousel-{{ ai_gen_id }} {
      padding: {{ block.settings.carousel_padding_vertical }}px {{ block.settings.desktop_padding }}px;
    }
  }
{% endstyle %}

<art-showcase-{{ ai_gen_id }} class="ai-art-showcase-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-art-showcase-container-{{ ai_gen_id }}">
    {% if block.settings.title != blank or block.settings.subtitle != blank %}
      <div class="ai-art-showcase-header-{{ ai_gen_id }}">
        {% if block.settings.title != blank %}
          <h2 class="ai-art-showcase-title-{{ ai_gen_id }}">{{ block.settings.title }}</h2>
        {% endif %}
        {% if block.settings.subtitle != blank %}
          <div class="ai-art-showcase-subtitle-{{ ai_gen_id }}">{{ block.settings.subtitle }}</div>
        {% endif %}
      </div>
    {% endif %}

    <div class="ai-art-showcase-carousel-wrapper-{{ ai_gen_id }}">
      <div class="ai-art-showcase-carousel-{{ ai_gen_id }}" data-carousel>
        {% for i in (1..6) %}
          {% liquid
            assign image_key = 'image_' | append: i
            assign title_key = 'card_title_' | append: i
            assign description_key = 'card_description_' | append: i
            assign image = block.settings[image_key]
            assign title = block.settings[title_key]
            assign description = block.settings[description_key]
          %}

          <div class="ai-art-showcase-card-{{ ai_gen_id }}">
            <div class="ai-art-showcase-image-wrapper-{{ ai_gen_id }}">
              {% if image %}
                <img
                  src="{{ image | image_url: width: 1200 }}"
                  alt="{{ image.alt | escape }}"
                  loading="lazy"
                  width="{{ image.width }}"
                  height="{{ image.height }}"
                  class="ai-art-showcase-image-{{ ai_gen_id }}"
                >
              {% else %}
                <div class="ai-art-showcase-image-placeholder-{{ ai_gen_id }}">
                  {{ 'hero-apparel-1' | placeholder_svg_tag }}
                </div>
              {% endif %}
              {% if block.settings.show_overlay %}
                <div class="ai-art-showcase-overlay-{{ ai_gen_id }}"></div>
              {% endif %}
            </div>
            {% if title != blank or description != blank %}
              <div class="ai-art-showcase-content-{{ ai_gen_id }}">
                {% if title != blank %}
                  <h3 class="ai-art-showcase-card-title-{{ ai_gen_id }}">{{ title }}</h3>
                {% endif %}
                {% if description != blank %}
                  <div class="ai-art-showcase-card-description-{{ ai_gen_id }}">{{ description }}</div>
                {% endif %}
              </div>
            {% endif %}
          </div>
        {% endfor %}
      </div>
    </div>

    {% if block.settings.show_navigation %}
      <div class="ai-art-showcase-nav-{{ ai_gen_id }}">
        <button
          class="ai-art-showcase-nav-button-{{ ai_gen_id }}"
          data-prev
          aria-label="Previous"
        >
          <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <polyline points="15 18 9 12 15 6"></polyline>
          </svg>
        </button>
        <div class="ai-art-showcase-dots-{{ ai_gen_id }}" data-dots></div>
        <button
          class="ai-art-showcase-nav-button-{{ ai_gen_id }}"
          data-next
          aria-label="Next"
        >
          <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <polyline points="9 18 15 12 9 6"></polyline>
          </svg>
        </button>
      </div>
    {% endif %}
  </div>
</art-showcase-{{ ai_gen_id }}>

<script>
  (function() {
    class ArtShowcase{{ ai_gen_id }} extends HTMLElement {
      constructor() {
        super();
        this.carousel = this.querySelector('[data-carousel]');
        this.prevButton = this.querySelector('[data-prev]');
        this.nextButton = this.querySelector('[data-next]');
        this.dotsContainer = this.querySelector('[data-dots]');
        this.cards = this.querySelectorAll('.ai-art-showcase-card-{{ ai_gen_id }}');
        this.currentIndex = 0;
      }

      connectedCallback() {
        if (!this.carousel) return;

        this.setupDots();
        this.setupNavigation();
        this.updateNavigation();

        this.carousel.addEventListener('scroll', () => {
          this.handleScroll();
        });
      }

      setupDots() {
        if (!this.dotsContainer) return;

        this.dotsContainer.innerHTML = '';
        this.cards.forEach((card, index) => {
          const dot = document.createElement('div');
          dot.className = 'ai-art-showcase-dot-{{ ai_gen_id }}';
          if (index === 0) dot.classList.add('active');
          dot.addEventListener('click', () => this.scrollToCard(index));
          this.dotsContainer.appendChild(dot);
        });
      }

      setupNavigation() {
        if (this.prevButton) {
          this.prevButton.addEventListener('click', () => this.scrollPrev());
        }
        if (this.nextButton) {
          this.nextButton.addEventListener('click', () => this.scrollNext());
        }
      }

      scrollPrev() {
        const cardWidth = this.cards[0].offsetWidth;
        const gap = {{ block.settings.card_gap }};
        this.carousel.scrollBy({
          left: -(cardWidth + gap),
          behavior: 'smooth'
        });
      }

      scrollNext() {
        const cardWidth = this.cards[0].offsetWidth;
        const gap = {{ block.settings.card_gap }};
        this.carousel.scrollBy({
          left: cardWidth + gap,
          behavior: 'smooth'
        });
      }

      scrollToCard(index) {
        const cardWidth = this.cards[0].offsetWidth;
        const gap = {{ block.settings.card_gap }};
        this.carousel.scrollTo({
          left: index * (cardWidth + gap),
          behavior: 'smooth'
        });
      }

      handleScroll() {
        const scrollLeft = this.carousel.scrollLeft;
        const cardWidth = this.cards[0].offsetWidth;
        const gap = {{ block.settings.card_gap }};
        const newIndex = Math.round(scrollLeft / (cardWidth + gap));

        if (newIndex !== this.currentIndex) {
          this.currentIndex = newIndex;
          this.updateDots();
          this.updateNavigation();
        }
      }

      updateDots() {
        if (!this.dotsContainer) return;

        const dots = this.dotsContainer.querySelectorAll('.ai-art-showcase-dot-{{ ai_gen_id }}');
        dots.forEach((dot, index) => {
          if (index === this.currentIndex) {
            dot.classList.add('active');
          } else {
            dot.classList.remove('active');
          }
        });
      }

      updateNavigation() {
        if (!this.prevButton || !this.nextButton) return;

        const maxScroll = this.carousel.scrollWidth - this.carousel.clientWidth;
        const scrollLeft = this.carousel.scrollLeft;

        this.prevButton.disabled = scrollLeft <= 0;
        this.nextButton.disabled = scrollLeft >= maxScroll - 1;
      }
    }

    customElements.define('art-showcase-{{ ai_gen_id }}', ArtShowcase{{ ai_gen_id }});
  })();
</script>

